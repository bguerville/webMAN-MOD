	<View id="seg_webman_redirect_usb_items">
		<Attributes>
			<Table key="remap_usb0">
				<Pair key="icon"><String>/dev_hdd0/game/XMBMANPLS/USRDIR/IMAGES/usbredirect.png</String></Pair>
				<Pair key="title"><String>Remap USB000 to HDD</String></Pair>
				<Pair key="info"><String>Redirect USB000 to /dev_hdd0/packages</String></Pair>
				<Pair key="module_name"><String>webrender_plugin</String></Pair>
				<Pair key="module_action"><String>http://localhost/remap.ps3/dev_usb000&to=/dev_hdd0/packages</String></Pair>
			</Table>
			<Table key="unmap_usb0">
				<Pair key="icon"><String>/dev_hdd0/game/XMBMANPLS/USRDIR/IMAGES/usbredirect.png</String></Pair>
				<Pair key="title"><String>Unmap USB000</String></Pair>
				<Pair key="info"><String>Restore redirection access to USB000</String></Pair>
				<Pair key="module_name"><String>webrender_plugin</String></Pair>
				<Pair key="module_action"><String>http://localhost/unmap.ps3/dev_usb000</String></Pair>
			</Table>
		</Attributes>
		<Items>
			<Item class="type:x-xmb/module-action" key="remap_usb0"		attr="remap_usb0"/>
			<Item class="type:x-xmb/module-action" key="unmap_usb0"		attr="unmap_usb0"/>
		</Items>
	</View>

